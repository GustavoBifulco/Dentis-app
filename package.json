{
  "name": "dentis-os",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -n \"VITE,HONO\" -c \"magenta,cyan\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "vite build && npm run server:build",
    "start": "npm run server:start",
    "server:build": "node esbuild.config.mjs",
    "server:start": "node dist-server/index.js",
    "server:dev": "npm run dev:server",
    "db:push": "drizzle-kit push",
    "db:seed": "npx tsx server/db/seed.ts",
    "db:reset": "npx tsx server/db/reset.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0a",
    "@clerk/clerk-react": "^5.0.0",
    "@clerk/clerk-sdk-node": "^5.0.0",
    "@hono/node-server": "^1.10.0",
    "@hono/zod-validator": "^0.2.1",
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^8.18.0",
    "@stripe/react-stripe-js": "^2.7.3",
    "@stripe/stripe-js": "^4.1.0",
    "@types/nodemailer": "^7.0.9",
    "@types/three": "^0.182.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^11.18.2",
    "hono": "^4.4.2",
    "lucide-react": "^0.344.0",
    "nodemailer": "^7.0.13",
    "openai": "^6.17.0",
    "papaparse": "^5.5.3",
    "pg": "^8.12.0",
    "postgres": "^3.4.8",
    "react": "18.3.1",
    "react-colorful": "^5.6.1",
    "react-cookie-consent": "^10.0.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "recharts": "^2.12.7",
    "stripe": "^14.0.0",
    "three": "^0.182.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.15.1",
    "typescript": "^5.5.3",
    "vite": "^7.3.1"
  }
}
